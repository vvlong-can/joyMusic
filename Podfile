platform :ios, '8.0'
# source 'https://github.com/CocoaPods/Specs.git'

def pods

  pod 'DOUAudioStreamer', '0.2.11'
  pod 'SDWebImage'
  pod 'GVUserDefaults'
  pod 'MBProgressHUD'
  pod 'Mantle', '1.5.5'
  pod 'AFNetworking', '~> 2.0'
  pod 'AVOSCloud'               # 数据存储、短信、云引擎调用等基础服务模块
  pod 'AVOSCloudIM'             # 实时通信模块
end

target 'joyMusic’ do
  pods
end
